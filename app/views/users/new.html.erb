<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>
<h1>新規投稿</h1>
<!-- form_for部分 -->
<%= form_for(@users) do |f| %>

    <h4>タイトル</h4>
    <%= f.text_field :title %>

    <h4>カテゴリー</h4>
    <%= f.text_field :category %>

    <h4>本文</h4>
    <%= f.text_area :body %>

    <%= f.submit '投稿' %>

<% end %>
